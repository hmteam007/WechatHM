<!-- session_item.hml -->
<div class="todo_child_view">
    <image class="todo_img" src="{{url}}"></image>
    <div class="todo_container">
        <text class="todo-name">{{name}}
        </text>
        <text if="{{msgFlag}}" class="todo-msg">{{msg}}
        </text>
    </div>
    <text if="{{dataFlag}}" class="todo-date">{{date}}
    </text>
</div>
