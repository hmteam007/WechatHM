<element src="../contacts/item/contacts_item.hml"></element>
<div class="container">
    <list class="todo-wraper" indexer="true">
        <list-item for="{{todolist}}" class="todo-item" onclick="clickAction({{$item}})">
            <contacts_item id="{{$item.id}}" name="{{$item.name}}" msg="{{$item.msg}}" date="{{$item.date}}"
                    url="{{$item.url}}"></contacts_item>
        </list-item>
    </list>
</div>
