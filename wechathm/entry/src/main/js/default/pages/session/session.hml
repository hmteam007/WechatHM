<element src="../../global/session_item/session_item.hml"></element>
<div class="container">
    <list class="todo-wraper">
        <list-item for="{{todolist}}" class="todo-item" onclick="clickAction({{$item}})">
            <session_item id="{{$item.id}}" name="{{$item.name}}" msg="{{$item.msg}}" date="{{$item.date}}"
                    url="{{$item.url}}"   msg-flag="{{$item.msgFlag}}" data-flag="{{$item.dataFlag}}"></session_item>
        </list-item>
    </list>
</div>